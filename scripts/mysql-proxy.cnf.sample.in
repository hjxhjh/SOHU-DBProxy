[mysql-proxy]
ro-address=0.0.0.0:3455
rw-address=0.0.0.0:3456
admin-address=127.0.0.1:3457
plugin-dir=lib/@PACKAGE_NAME@/plugins/
config-xml=etc/mysql-proxy.xml
plugins=proxy,admin
admin-username=admin
admin-password=admin
pid-file=var/log/mysql-proxy.pid
log-file=var/log/mysql-proxy.log
log-level=warning
keepalive=true
daemon=true
event-threads=8
max-open-files=60000
max-core-size=unlimited
dbproxy-collation=utf8_general_ci
